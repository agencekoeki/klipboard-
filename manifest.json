{
  "manifest_version": 3,
  "name": "Klipboard",
  "version": "1.1",
  "description": "Extension Koeki pour copier automatiquement les donn√©es SEO de thot-seo.fr",
  "permissions": [
    "activeTab",
    "clipboardWrite"
  ],
  "content_scripts": [
    {
      "matches": ["*://thot-seo.fr/*"],
      "js": [
        "sectionExpander.js",
        "nlpManager.js", 
        "promptsManager.js",
        "dataExtractor.js",
        "content.js"
      ],
      "css": ["styles.css"]
    }
  ]
}