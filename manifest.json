{
  "manifest_version": 3,
  "name": "Klipboard",
  "version": "1.0",
  "description": "Extension Koeki pour copier automatiquement les donn√©es SEO de thot-seo.fr",
  "permissions": [
    "activeTab",
    "clipboardWrite"
  ],
  "content_scripts": [
    {
      "matches": ["*://thot-seo.fr/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Klipboard"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}